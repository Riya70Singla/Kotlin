fun main(){
 println("continue statement")
for(i in 1..3){
    for(j in 1..3){
        if(i==2 && j==2)
        continue
    println("$i $j")
    }
}
//to break out of outer layer use labelled for statement
myLoop@ for(i in 1..3){
    for(j in 1..3){
        if(i==2 && j==2)
        continue@myLoop
    println("$i $j")
   
    }
}
}
